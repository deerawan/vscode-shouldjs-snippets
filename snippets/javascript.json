{
  "containDeep": {
    "prefix": "shcd",
    "body": "$1.should.containDeep($2);",
    "description": "Deep object equality comparison"
  },
  "eql": {
    "prefix": "sheql",
    "body": "should($1).be.eql($2);",
    "description": "Deep object equality comparison"
  },
  "eqlNot": {
    "prefix": "shnoteql",
    "body": "should($1).not.be.eql($2);",
    "description": "Deep object equality comparison (NOT)"
  },
  "equal": {
    "prefix": "sheq",
    "body": "should($1).be.equal($2);",
    "description": "Exact comparison using ==="
  },
  "equalOneOf": {
    "prefix": "shone",
    "body": "should($1).be.oneOf($2)",
    "description": "Exact comparison using .eql to be one of supplied objects."
  },
  "exactly": {
    "prefix": "shex",
    "body": "should($1).be.exactly($2);",
    "description": "Aliases Assertion#equal"
  }
}